package day1prac.라이브러리만들기;

//1. 별을 3개 출력하는 기능
//2. 특정문자를 3번 출력하느 기능 Char =>'a'
//3. 당신의 코드를 3번 실행하는 기능 ->인터페이스 활용

public class TestLib {
	
	public void star3() {
		for(int i=0;i<3;i++) {
			System.out.print("* ");
		}
		System.out.println();
	}
	
	public void printChar(char a) {
		
		for(int i=0;i<3;i++) {
			System.out.println(a);
		}
	}
	
	//라이브러리를 제공할 때
	//결정할 수 없는 코드가 생김 => 
	//인터페이스 활용함(라이브러리를 제공하는 사람이)
	
	//interface 코드 { void run(); }
	
	public void runCode() {
		for(int i=0;i<3;i++) {
			System.out.println();
		}
	}
	
}
