package day2prac.함수형인터페이스2;

import java.util.function.Consumer;

public class 실습 {
	public static void main(String[] args) {
		Runnable r =() ->{
			System.out.println("건물주되기");
		};
		r.run();
		
		Consumer<Integer> c = (money)->{
			System.out.println(money+" 먹는데 다 썼음");
		};
		
		c.accept(2100000000);
	}
}
