package day3Prac.stream1;

import java.util.Arrays;
import java.util.stream.IntStream;

public class Ex02 {

	public static void main(String[] args) {
		//객체
		
		String[] arr = new String[5];
		arr[0]="hi";
		arr[1]="hello";
		arr[2]="bye";
		arr[3]="hi^^^^";
		arr[4]="hello^^^^^^^";
		
		Arrays.sort(arr);
		
		
	}

}
