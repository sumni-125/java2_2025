package day5Prac.애너테이션실습;

public class Emlpoyee {
	
	String name;
	
	
	String offDay;


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getOffDay() {
		return offDay;
	}


	public void setOffDay(String offDay) {
		this.offDay = offDay;
	}


	@Override
	public String toString() {
		return "Emlpoyee [name=" + name + ", offDay=" + offDay + "]";
	}


	public Emlpoyee(String name, String offDay) {
		this.name = name;
		this.offDay = offDay;
	}
	
	
	public Emlpoyee() {
		// TODO Auto-generated constructor stub
	}
}
